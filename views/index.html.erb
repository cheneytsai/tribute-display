<!DOCTYPE html>
<html>
  <head>
    <title>Tribute Display Demo</title>
    <link href="assets/css/bootstrap.min.css" rel="stylesheet" media="screen">
    <link href="assets/css/application.css" rel="stylesheet" media="screen">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
  </head>
  <body>

    <div id="output-text">
      <div class="stat">
        <h2 id="served-count"></h2>
        <p>People Served</p>
      </div>
<!--       <div class="stat">
        <h2 id="raised-count"></h2>
        <p>Raised</p>
      </div> -->
      <div class="stat">
        <h2 id="pumped-count"></h2>
        <p>Liters Pumped</p>
      </div>
    </div>

    <form id="input-form" class="form-inline">
      <div class="form-group">
        <input id="input-liters" type="text" class="form-control" placeholder="Enter liters" autofocus />
      </div>
      <button class="btn btn-primary" type="submit">Send</button>
    </form>

    <script type="text/javascript" src="assets/js/jquery-2.0.3.min.js"></script>
    <script type="text/javascript" src="assets/js/application.js"></script>
  </body>
</html>
